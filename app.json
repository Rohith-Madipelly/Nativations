{
  "expo": {
    "newArchEnabled": true,
    "name": "Satya Sadhna",
    "slug": "Navigations",
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "vardhaman.satyasadhnaOne",
      
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "vardhaman.satyasadhnaOne",
      "versionCode": 7
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7c804e7a-7842-4219-9ab3-fb5aca0c2563"
      }
    },
    "plugins": [
      "expo-font",
      "expo-video"
    ],
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-loading-spinner-overlay",
          "react-native-snap-carousel",
          "@reduxjs/toolkit",
          "doctor",
          "form-data",
          "react-native-select-dropdown",
          "react-native-ytdl",
          "react-redux",
          "yup"
        ],
        "listUnknownPackages": false
      }
    }
  }
}